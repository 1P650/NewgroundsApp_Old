<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <declare-styleable name="Dashboard">
        <attr name="labelVisible" format="boolean"/>
        <attr name="labelText" format="string" />
        <attr name="labelIcon" format="reference" />
        <attr name="labelMenu" format="reference" />
        <attr name="labelMenuType" format="integer" >
            <enum name="strings" value="0" />
            <enum name="menu" value="1" />
            <enum name="contextMenu" value="2"/>
            <enum name="complexContextMenu" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="ProfileWidget">
        <attr name="showNotification" format="boolean" />
    </declare-styleable>
</resources>